{
  "productName": "watery",
  "version": "0.1.0",
  "identifier": "com.watery.app",
  "build": {
    "beforeDevCommand": "pnpm dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "pnpm build",
    "frontendDist": "../dist"
  },
  
  
  "plugins": {
    "shell": {
    },
    "deep-link": {
      "desktop": {
        "schemes": ["watery", "mailto"]
      }
    }
  },
  
  "app": {
    "windows": [
      {
        "title": "watery",
        "width": 800,
        "height": 600
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
